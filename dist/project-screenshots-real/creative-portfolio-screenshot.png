<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f0f23;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a2e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2a2a3e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e1e2e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fondo -->
  <rect width="100%" height="100%" fill="url(#bg)"/>
  
  <!-- Header con logo -->
  <g transform="translate(20, 20)">
    <text x="0" y="30" font-family="Arial, sans-serif" font-size="24" fill="white" font-weight="bold">
      Creative Studio
    </text>
    <text x="0" y="45" font-family="Arial, sans-serif" font-size="12" fill="#06b6d4">
      Diseño & Desarrollo
    </text>
  </g>
  
  <!-- Navegación -->
  <g transform="translate(20, 70)">
    <rect x="0" y="0" width="60" height="30" rx="15" fill="#06b6d4"/>
    <text x="30" y="20" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle">Inicio</text>
    
    <rect x="70" y="0" width="60" height="30" rx="15" fill="transparent" stroke="#4a4a5e" stroke-width="1"/>
    <text x="100" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Servicios</text>
    
    <rect x="140" y="0" width="60" height="30" rx="15" fill="transparent" stroke="#4a4a5e" stroke-width="1"/>
    <text x="170" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Portfolio</text>
    
    <rect x="210" y="0" width="60" height="30" rx="15" fill="transparent" stroke="#4a4a5e" stroke-width="1"/>
    <text x="240" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Contacto</text>
  </g>
  
  <!-- Hero Section -->
  <g transform="translate(20, 120)">
    <text x="0" y="30" font-family="Arial, sans-serif" font-size="28" fill="white" font-weight="bold">
      Creamos Experiencias
    </text>
    <text x="0" y="55" font-family="Arial, sans-serif" font-size="28" fill="white" font-weight="bold">
      Digitales Únicas
    </text>
    <text x="0" y="80" font-family="Arial, sans-serif" font-size="14" fill="#888">
      Especialistas en diseño web, branding y desarrollo de aplicaciones
    </text>
    <text x="0" y="100" font-family="Arial, sans-serif" font-size="14" fill="#888">
      que conectan con tu audiencia.
    </text>
    
    <!-- Botones CTA -->
    <rect x="0" y="120" width="120" height="35" rx="17" fill="#06b6d4"/>
    <text x="60" y="142" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle">Ver Proyectos</text>
    
    <rect x="130" y="120" width="120" height="35" rx="17" fill="transparent" stroke="#4a4a5e" stroke-width="1"/>
    <text x="190" y="142" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Contactar</text>
  </g>
  
  <!-- Servicios destacados -->
  <g transform="translate(20, 200)">
    <rect x="0" y="0" width="100" height="80" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="50" y="20" font-family="Arial, sans-serif" font-size="10" fill="#06b6d4" text-anchor="middle">🎨</text>
    <text x="50" y="35" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle">Diseño Web</text>
    <text x="50" y="50" font-family="Arial, sans-serif" font-size="8" fill="#888" text-anchor="middle">Interfaces modernas</text>
    <text x="50" y="65" font-family="Arial, sans-serif" font-size="8" fill="#888" text-anchor="middle">y responsivas</text>
    
    <rect x="110" y="0" width="100" height="80" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="160" y="20" font-family="Arial, sans-serif" font-size="10" fill="#06b6d4" text-anchor="middle">💻</text>
    <text x="160" y="35" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle">Desarrollo</text>
    <text x="160" y="50" font-family="Arial, sans-serif" font-size="8" fill="#888" text-anchor="middle">Aplicaciones web</text>
    <text x="160" y="65" font-family="Arial, sans-serif" font-size="8" fill="#888" text-anchor="middle">personalizadas</text>
    
    <rect x="220" y="0" width="100" height="80" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="270" y="20" font-family="Arial, sans-serif" font-size="10" fill="#06b6d4" text-anchor="middle">🚀</text>
    <text x="270" y="35" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle">Branding</text>
    <text x="270" y="50" font-family="Arial, sans-serif" font-size="8" fill="#888" text-anchor="middle">Identidad visual</text>
    <text x="270" y="65" font-family="Arial, sans-serif" font-size="8" fill="#888" text-anchor="middle">y marca</text>
  </g>
</svg>