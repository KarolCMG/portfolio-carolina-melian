<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#334155;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fondo -->
  <rect width="100%" height="100%" fill="url(#bg)"/>
  
  <!-- Header -->
  <g transform="translate(20, 20)">
    <text x="0" y="25" font-family="Arial, sans-serif" font-size="20" fill="white" font-weight="bold">
      DataViz Pro
    </text>
    <text x="0" y="40" font-family="Arial, sans-serif" font-size="12" fill="#10b981">
      Analytics Dashboard
    </text>
  </g>
  
  <!-- Filtros de fecha -->
  <g transform="translate(20, 60)">
    <rect x="0" y="0" width="80" height="25" rx="12" fill="#10b981"/>
    <text x="40" y="16" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle">Últimos 7 días</text>
    
    <rect x="90" y="0" width="60" height="25" rx="12" fill="transparent" stroke="#4a4a5e" stroke-width="1"/>
    <text x="120" y="16" font-family="Arial, sans-serif" font-size="10" fill="#888" text-anchor="middle">30 días</text>
    
    <rect x="160" y="0" width="60" height="25" rx="12" fill="transparent" stroke="#4a4a5e" stroke-width="1"/>
    <text x="190" y="16" font-family="Arial, sans-serif" font-size="10" fill="#888" text-anchor="middle">90 días</text>
  </g>
  
  <!-- Métricas principales -->
  <g transform="translate(20, 100)">
    <rect x="0" y="0" width="80" height="60" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="40" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Usuarios</text>
    <text x="40" y="40" font-family="Arial, sans-serif" font-size="20" fill="white" text-anchor="middle">12,543</text>
    <text x="40" y="55" font-family="Arial, sans-serif" font-size="10" fill="#10b981" text-anchor="middle">↗ +12.5%</text>
    
    <rect x="90" y="0" width="80" height="60" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="130" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Ventas</text>
    <text x="130" y="40" font-family="Arial, sans-serif" font-size="20" fill="white" text-anchor="middle">$45,231</text>
    <text x="130" y="55" font-family="Arial, sans-serif" font-size="10" fill="#10b981" text-anchor="middle">↗ +8.2%</text>
    
    <rect x="180" y="0" width="80" height="60" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="220" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Conversión</text>
    <text x="220" y="40" font-family="Arial, sans-serif" font-size="20" fill="white" text-anchor="middle">3.24%</text>
    <text x="220" y="55" font-family="Arial, sans-serif" font-size="10" fill="#ef4444" text-anchor="middle">↘ -2.1%</text>
    
    <rect x="270" y="0" width="80" height="60" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="310" y="20" font-family="Arial, sans-serif" font-size="12" fill="#888" text-anchor="middle">Ingresos</text>
    <text x="310" y="40" font-family="Arial, sans-serif" font-size="20" fill="white" text-anchor="middle">$89,456</text>
    <text x="310" y="55" font-family="Arial, sans-serif" font-size="10" fill="#10b981" text-anchor="middle">↗ +15.3%</text>
  </g>
  
  <!-- Gráfico principal -->
  <g transform="translate(20, 180)">
    <rect x="0" y="0" width="200" height="100" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="10" y="15" font-family="Arial, sans-serif" font-size="12" fill="white">Ventas por Día</text>
    
    <!-- Barras del gráfico -->
    <rect x="20" y="80" width="15" height="20" fill="#10b981"/>
    <rect x="40" y="70" width="15" height="30" fill="#10b981"/>
    <rect x="60" y="60" width="15" height="40" fill="#10b981"/>
    <rect x="80" y="50" width="15" height="50" fill="#10b981"/>
    <rect x="100" y="40" width="15" height="60" fill="#10b981"/>
    <rect x="120" y="30" width="15" height="70" fill="#10b981"/>
    <rect x="140" y="35" width="15" height="65" fill="#10b981"/>
    <rect x="160" y="45" width="15" height="55" fill="#10b981"/>
    
    <!-- Ejes -->
    <text x="20" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">L</text>
    <text x="40" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">M</text>
    <text x="60" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">M</text>
    <text x="80" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">J</text>
    <text x="100" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">V</text>
    <text x="120" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">S</text>
    <text x="140" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">D</text>
    <text x="160" y="95" font-family="Arial, sans-serif" font-size="8" fill="#888">L</text>
  </g>
  
  <!-- Gráfico circular -->
  <g transform="translate(240, 180)">
    <rect x="0" y="0" width="120" height="100" rx="8" fill="url(#card)" stroke="#4a4a5e" stroke-width="1"/>
    <text x="10" y="15" font-family="Arial, sans-serif" font-size="12" fill="white">Tráfico por Fuente</text>
    
    <!-- Círculo del gráfico -->
    <circle cx="60" cy="60" r="25" fill="none" stroke="#10b981" stroke-width="8" stroke-dasharray="78.5 78.5" stroke-dashoffset="0"/>
    <circle cx="60" cy="60" r="25" fill="none" stroke="#3b82f6" stroke-width="8" stroke-dasharray="78.5 78.5" stroke-dashoffset="39.25"/>
    <circle cx="60" cy="60" r="25" fill="none" stroke="#f59e0b" stroke-width="8" stroke-dasharray="78.5 78.5" stroke-dashoffset="58.875"/>
    
    <!-- Leyenda -->
    <g transform="translate(10, 80)">
      <rect x="0" y="0" width="8" height="8" fill="#10b981"/>
      <text x="12" y="7" font-family="Arial, sans-serif" font-size="8" fill="#888">Directo 40%</text>
      
      <rect x="0" y="12" width="8" height="8" fill="#3b82f6"/>
      <text x="12" y="19" font-family="Arial, sans-serif" font-size="8" fill="#888">Social 30%</text>
      
      <rect x="0" y="24" width="8" height="8" fill="#f59e0b"/>
      <text x="12" y="31" font-family="Arial, sans-serif" font-size="8" fill="#888">Email 30%</text>
    </g>
  </g>
</svg>